[
  {
    "description": "negative pick and offset",
    "input": {"pick": -1, "offset": -1},
    "expect": { 
      "length": -1,
      "errorMessageCollection": [
        "pick can not be negative",
        "offset can not be negative"
      ]
    }
  },
  {
    "description": "pick equal 0, pick undefined",
    "input": {"pick": 0},
    "expect": { 
      "length": -1,
      "errorMessageCollection": [
        "pick can not be equal 0",
        "offset can not be undefined or null"
      ]
    }
  },
  {
    "description": "pick is string, ofset 1",
    "input": {"pick": "UNDEFINED", "offset": 1},
    "expect": { 
      "length": -1,
      "errorMessageCollection": [
        "pick can not be string, but expect number",
        "offset can not be negative"
      ]
    }
  },
  {
    "description": "pick 1, offset string",
    "input": {"pick": 1, "offset": "TEST STRING"},
    "expect": {
      "length": -1,
      "errorMessageCollection": [
        "offset can not be string, but expect number"
      ]
    }
  },
  {
    "description": "pick 1, offset: 20",
    "input": {"pick": 1, "offset": 20},
    "expect": {
      "length": 1,
      "errorMessageCollection": []
    }
  },
  {
    "description": "pick 10, offset 0",
    "input": {"pick": 10, "offset": 0},
    "expect": {
      "length": 10,
      "errorMessageCollection": []
    }
  }
]
